{
  "name": "nyt-react-app",
  "version": "0.1.0",
  "engines": {
    "node": "8.9.0"
  },
  "private": true,
  "scripts": {
    "heroku-prebuild": "rm -rf public/ && mkdir public",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"node server.js\" \"cd client && yarn start\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^3.6.0",
    "cookie-parser": "~1.4.3",
    "date-fns": "^1.29.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "^1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.2.1",
    "morgan": "~1.9.0",
    "react-date-picker": "^6.10.4",
    "react-router-dom": "^4.3.1"
  }
}
